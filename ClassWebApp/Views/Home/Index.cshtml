@{
}

<h1>This is my list of Students</h1>
<table border="1">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
@{
    @foreach(var s in ViewBag.ListOfStudents)
    {
        <td> @s.FirstName </td>
        <td> @s.LastName </td>
    }
}
</table>